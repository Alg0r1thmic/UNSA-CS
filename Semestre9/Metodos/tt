procedure camarera(){
	again:	cl_to_cam?saludo1;
			cam_to_cl!saludo2;
			cl_to_cam?pedido;
			cam_to_coc!pedido;
			coc_to_cam?comidaLista;
			cam_to_cl!comidaLista;
			cl_to_cam?pedirCuenta;
			cam_to_cl!cuenta;
			cl_to_cam?dinero;
			cam_to_cl!despedida1;
			cl_to_cam?despedida2;
			goto again
}